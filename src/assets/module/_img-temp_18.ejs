<%# shopify webPなし retinaなし lazyなし%>
<picture>
  <source
    srcset="{{ '<%= imgInfo.path %>.<%= imgInfo.type %>' | file_url }}"
    media="(min-width: 768px)"
  >
  <source
    srcset="{{ '<%= imgInfo.path %>_sp.<%= imgInfo.type %>' | file_url }}"
  >
  <img
    src="{{ '<%= imgInfo.path %>.<%= imgInfo.type %>' | file_url }}"
    alt="<%= imgInfo.alt %>"
  >
</picture>
