<%# svg画像直接 代替はwebP%>
<picture>
  <source
    data-srcset="assets/images/<%= imgInfo.path %>.svg"
  >
  <source
    data-srcset="assets/images/<%= imgInfo.path %>.webp"
    type="image/webp"
  >
  <img
    data-src="assets/images/<%= imgInfo.path %>.<%= imgInfo.type %>"
    alt="<%= imgInfo.alt %>"
    class="lazyload"
  >
</picture>
