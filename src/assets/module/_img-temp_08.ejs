<%# lazyloadなし%>
<picture>
  <source
    srcset="assets/images/<%= imgInfo.path %>.webp"
    media="(min-width: 768px)"
    type="image/webp"
  >
  <source
    srcset="assets/images/<%= imgInfo.path %>.<%= imgInfo.type %>"
    media="(min-width: 768px)"
  >
  <source
    srcset="assets/images/<%= imgInfo.path %>_sp.webp 1x,assets/images/<%= imgInfo.path %>_sp@2x.webp 2x,assets/images/<%= imgInfo.path %>_sp@3x.webp 3x"
    type="image/webp"
  >
  <source
    srcset="assets/images/<%= imgInfo.path %>_sp.<%= imgInfo.type %> 1x,assets/images/<%= imgInfo.path %>_sp@2x.<%= imgInfo.type %> 2x,assets/images/<%= imgInfo.path %>_sp@3x.<%= imgInfo.type %> 3x"
  >
  <img
    src="assets/images/<%= imgInfo.path %>.<%= imgInfo.type %>"
    alt="<%= imgInfo.alt %>"
  >
</picture>
