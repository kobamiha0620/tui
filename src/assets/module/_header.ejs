            <li class="label">Subscription</li>
            <li><a href="booster-oil-s.html">BOOSTER OIL</a></li>
            <li><a href="moisture-serum-s.html">MOISTURE SERUM</a></li>
            <li><a href="set-package-s.html">SET PACKAGE</a></li>
          </ul>
          <!-- /.productNav -->
        </div>
        <!-- /.inner -->
      </div>
      <!-- /.productNavWrapper -->
    </div>
    <!-- /.col-nav -->
    <% } else {%>
    <div class="col-nav">
      <p class="parent"><a href="{{ routes.root_url }}pages/products">View the products</a></p>
      <div class="productNavWrapper">
        <ul class="productNav">
          <li><a href="{{ routes.root_url }}products/booster-oil">BOOSTER OIL</a></li>
          <li><a href="{{ routes.root_url }}products/moisture-serum">MOISTURE SERUM</a></li>
          <li><a href="{{ routes.root_url }}products/set-package">SET PACKAGE</a></li>
