        <ul class="productNav">
          <li class="label">Subscription</li>
          <li><a href="{{ routes.root_url }}products/booster-oil-s">BOOSTER OIL</a></li>
          <li><a href="{{ routes.root_url }}products/moisture-serum-s">MOISTURE SERUM</a></li>
          <li><a href="{{ routes.root_url }}products/set-package-s">SET PACKAGE</a></li>
        </ul>
        <!-- /.productNav -->
      </div>
      <!-- /.productNavWrapper -->
    </div>
    <!-- /.col-nav -->
    <% }%>
    <div class="col-iconNav">
      <ul>
        <li class="cartNav">
          <% if(pageinit.shopify==='true'){%>
          <a href="{{ routes.cart_url }}">
            <% var imgInfo = {path:'icon-cart',type:'svg',alt:'tui'};%>
            <% if(pageinit.shopify!=='true'){%>
              <%- include(pageinit.path + '_img-temp_12.ejs', {imgInfo:imgInfo});%>
            <% } else {%>
              <%- include(pageinit.path + '_img-temp_13.ejs', {imgInfo:imgInfo});%>
            <% }%>
          </a>
          <% } else {%>
          <a href="javascript:void(0);">
            <% var imgInfo = {path:'icon-cart',type:'svg',alt:'tui'};%>
            <% if(pageinit.shopify!=='true'){%>
              <%- include(pageinit.path + '_img-temp_12.ejs', {imgInfo:imgInfo});%>
            <% } else {%>
              <%- include(pageinit.path + '_img-temp_13.ejs', {imgInfo:imgInfo});%>
            <% }%>
          </a>
          <% }%>
