<header>
  <div class="inner">
    <div class="col-logo">
      <p class="logo tx headMenu">
        <% if(pageinit.shopify!=='true'){%>
        <a href="./">
        <% var imgInfo = {path:'logo-tui_01',type:'svg',alt:'tui'};%>
        <% if(pageinit.shopify!=='true'){%>
          <%- include(pageinit.path + '_img-temp_12.ejs', {imgInfo:imgInfo});%>
        <% } else {%>
          <%- include(pageinit.path + '_img-temp_13.ejs', {imgInfo:imgInfo});%>
        <% }%>
        </a>
        <% } else {%>
        <a href="{{ routes.root_url }}">
        <% var imgInfo = {path:'logo-tui_01',type:'svg',alt:'tui'};%>
        <% if(pageinit.shopify!=='true'){%>
          <%- include(pageinit.path + '_img-temp_12.ejs', {imgInfo:imgInfo});%>
        <% } else {%>
          <%- include(pageinit.path + '_img-temp_13.ejs', {imgInfo:imgInfo});%>
        <% }%>
        </a>
        <% }%>
      </p>
    </div>
    <!-- /.col-logo -->
    <% if(pageinit.shopify!=='true'){%>
    <div class="col-nav">
      <p class="parent"><a href="products.html">View the products</a></p>
      <div class="productNavWrapper">
